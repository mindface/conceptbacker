<%= form_with(model: imaring) do |form| %>
  <% if imaring.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(imaring.errors.count, "error") %> prohibited this imaring from being saved:</h2>

      <ul>
        <% imaring.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :name, style: "display: block" %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :conectid, style: "display: block" %>
    <%= form.text_field :conectid %>
  </div>

  <div>
    <%= form.label :images, "画像" %>
    <%= form.file_field :images, multiple: true %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
